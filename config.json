[
    {
        "schedule_id": "daily_social_distancing",
        "hours": [
            21
        ],
        "context":[],
        "randomize": 2,
        "start_date": "04-01-2020",
        "expiration": 240,
        "end_date": "06-30-2020",
        "notification_title": "AWARE ESM",
        "notification_body": "Social Distancing Study: Tell us how your day went today",
        "interface": 0,
        "esms": [
            {
                "esm": {
                    "esm_type": 2,
                    "esm_title": "I am currently self-isolating (e.g., staying home) as a result of COVID-19.",
                    "esm_instructions": "",
                    "esm_radios": [
                        "No, not at all",
                        "Yes, slightly (leaving my house but less than usual)",
                        "Yes, I am not leaving my house at all",
                        "Yes, I am isolating myself from everyone including people in my house"
                    ],
                    "esm_submit": "Next",
                    "esm_expiration_threshold": 0,
                    "esm_trigger": "esm_basic_test_2",
                    "esm_na": -1
                }
            },
            {
                "esm": {
                    "esm_type": 6,
                    "esm_title": "How many people did you have in-person interactions with today?",
                    "esm_scale_min": 0,
                    "esm_scale_max": 15,
                    "esm_scale_start": 2,
                    "esm_scale_max_label": "15",
                    "esm_scale_min_label": "0",
                    "esm_scale_step": 1,
                    "esm_submit": "Submit",
                    "esm_expiration_threshold": 0,
                    "esm_trigger": "esm_ppl_met",
                    "esm_na": -1
                }
            },
            {
                "esm": {
                    "esm_type": 6,
                    "esm_title": "How many people did you have phone, online or other social-media based interactions with today??",
                    "esm_scale_min": 0,
                    "esm_scale_max": 10,
                    "esm_scale_start": 2,
                    "esm_scale_max_label": "30",
                    "esm_scale_min_label": "0",
                    "esm_scale_step": 1,
                    "esm_submit": "Submit",
                    "esm_expiration_threshold": 0,
                    "esm_trigger": "esm_ppl_met",
                    "esm_na": -1
                }
            },
            {
                "esm": {
                    "esm_type": 4,
                    "esm_title": "I felt outcasted and alone in my interactions with others today.",
                    "esm_instructions": "",
                    "esm_likert_max": 7,
                    "esm_likert_max_label": "Strongly Agree",
                    "esm_likert_min_label": "Strongly Disagree",
                    "esm_likert_step": 1,
                    "esm_submit": "Next",
                    "esm_expiration_threshold": 60,
                    "esm_trigger": "esm_basic_test_4"
                }
            },
            {
                "esm": {
                    "esm_type": 4,
                    "esm_title": "I felt understood/cared for by others today",
                    "esm_instructions": "",
                    "esm_likert_max": 7,
                    "esm_likert_max_label": "Strongly Agree",
                    "esm_likert_min_label": "Strongly Disagree",
                    "esm_likert_step": 1,
                    "esm_submit": "Next",
                    "esm_expiration_threshold": 60,
                    "esm_trigger": "esm_basic_test_4"
                }
            },
            {
                "esm": {
                    "esm_type": 4,
                    "esm_title": "I over-analysed situations to the point where itâ€™s unhelpful to me",
                    "esm_likert_max": 7,
                    "esm_likert_max_label": "Very frequenty today",
                    "esm_likert_min_label": "Almost never",
                    "esm_likert_step": 1,
                    "esm_submit": "Next",
                    "esm_expiration_threshold": 60,
                    "esm_trigger": "esm_basic_test_4"
                }
            },
            {
                "esm": {
                    "esm_type": 4,
                    "esm_title": "My thoughts cause me distress or emotional pain today",
                    "esm_likert_max": 7,
                    "esm_likert_max_label": "Very frequenty today",
                    "esm_likert_min_label": "Almost never",
                    "esm_likert_step": 1,
                    "esm_submit": "Next",
                    "esm_expiration_threshold": 60,
                    "esm_trigger": "esm_basic_test_4"
                }
            },
            {
                "esm": {
                    "esm_type": 4,
                    "esm_title": "I find myself preoccupied with the future or the past",
                    "esm_likert_max": 7,
                    "esm_likert_max_label": "Very frequenty today",
                    "esm_likert_min_label": "Almost never",
                    "esm_likert_step": 1,
                    "esm_submit": "Next",
                    "esm_expiration_threshold": 60,
                    "esm_trigger": "esm_basic_test_4"
                }
            }
    
            
        ]
    },
